{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "~7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "alterphp/easyadmin-extension-bundle": "^2.0",
        "api-platform/core": "^2.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.12",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "helios-ag/fm-elfinder-bundle": "^9.1",
        "itk-dev/config-bundle": "^3.0",
        "itk-dev/database-bundle": "^1.2",
        "itk-dev/web-accessibility-statement-bundle": "^1.0",
        "lexik/jwt-authentication-bundle": "^2.5",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^4.3",
        "phpoffice/phpspreadsheet": "^1.6",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.3.*",
        "symfony/cache": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.3.*",
        "symfony/property-access": "4.3.*",
        "symfony/property-info": "4.3.*",
        "symfony/proxy-manager-bridge": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer": "4.3.*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/web-link": "4.3.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.3.*",
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "4.3.*",
        "symfony/css-selector": "4.3.*",
        "symfony/debug-bundle": "4.3.*",
        "symfony/maker-bundle": "^1.7",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "4.3.*",
        "symfony/web-profiler-bundle": "4.3.*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "component-dir": "public/components",
        "allow-plugins": {
            "ocramius/package-versions": true,
            "robloach/component-installer": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --release=standard --tag=4.6.0 --clear=drop --exclude=samples": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check-coding-standards/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix --dry-run"
        ],
        "check-coding-standards": [
            "@check-coding-standards/php-cs-fixer"
        ],
        "apply-coding-standards/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix"
        ],
        "apply-coding-standards": [
            "@apply-coding-standards/php-cs-fixer"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "twig/twig": "2.6.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
